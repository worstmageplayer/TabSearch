{
  "manifest_version": 3,
  "name": "Fuzzy Tab Search",
  "version": "1.0",
  "description": "Quick fuzzy tab switcher",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/icon.png"
    }
  },
  "permissions": ["tabs", "activeTab", "commands"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Open fuzzy tab search"
    }
  },
  "icons": {
    "32": "icons/icon.png"
  }
}
